{
  "name": "fastify-prisma",
  "version": "0.0.1",
  "scripts": {
    "dev": "pnpm build:watch & pnpm start:watch",
    "build": "swc src -d dist -s --config-file .swcrc",
    "start": "node dist/index.js",
    "build:watch": "swc src -d dist -s -w --config-file .swcrc",
    "start:watch": "nodemon dist/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.17",
    "@types/node": "^18.11.10",
    "chokidar": "^3.5.3",
    "nodemon": "^2.0.20",
    "prisma": "^4.7.1"
  },
  "dependencies": {
    "@prisma/client": "^4.7.1",
    "fastify": "^4.10.2"
  }
}
